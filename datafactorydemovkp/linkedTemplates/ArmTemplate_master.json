{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorydemovkp"
		},
		"ls_covidblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covidblob'"
		},
		"ls_covidgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covidgen2'"
		},
		"ls_covidgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingvkpgen2.dfs.core.windows.net"
		},
		"EventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4cedc5dd-e3ad-468d-bf66-32e31bdb9148/resourceGroups/1-e4f2574d-playground-sandbox/providers/Microsoft.Storage/storageAccounts/covidreportingvkp"
		},
		"ls_httpcoviddata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorydemovkp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covidblob_connectionString": {
						"value": "[parameters('ls_covidblob_connectionString')]"
					},
					"ls_covidgen2_accountKey": {
						"value": "[parameters('ls_covidgen2_accountKey')]"
					},
					"ls_covidgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_covidgen2_properties_typeProperties_url')]"
					},
					"ls_httpcoviddata_properties_typeProperties_url": {
						"value": "[parameters('ls_httpcoviddata_properties_typeProperties_url')]"
					},
					"EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}